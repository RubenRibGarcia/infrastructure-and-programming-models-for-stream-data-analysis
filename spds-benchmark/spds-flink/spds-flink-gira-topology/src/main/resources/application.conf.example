topology {
    parallelism = 24
    until_phase = INGESTION
}
rabbitmq {
    host = "rabbitmq"
    port = 5672
    username = "<rabbitmq_username>"
    password = "<rabbitmq_password>"
}
redis {
    mocked = false
    host = "redis"
    port = 6379
}
metrics {
    statsd {
        host = localhost
        port = 8125
        agent = TELEGRAF
    }
}