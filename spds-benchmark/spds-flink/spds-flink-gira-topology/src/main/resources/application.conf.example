rabbitmq {
    host = "rabbitmq"
    port = 5672
    virtual_host = "/"
    username = "<rabbitmq_username>"
    password = "<rabbitmq_password>"
}
redis {
    host = "redis"
    port = 6379
    min_idle = 0
    max_idle = 10
    max_total = 10
}
metrics {
    statsd {
        host = localhost
        port = 8125
        agent = TELEGRAF
    }
}