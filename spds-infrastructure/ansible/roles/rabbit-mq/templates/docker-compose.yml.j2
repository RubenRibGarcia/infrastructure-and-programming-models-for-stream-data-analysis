version: "3.7"

services:
  rabbitmq:
    image: rabbitmq:3.8.5-management-alpine
    container_name: rabbitmq
    restart: always
    environment:
      - "RABBITMQ_DEFAULT_USER=admin"
      - "RABBITMQ_DEFAULT_PASS=admin"
    ports:
      - "5672:5672"
      - "8080:15672"

networks:
  default:
    external:
      name: spds-network